{
  "name": "freshxmind",
  "version": "1.0.0",
  "description": "Fresh X Mind is a Wordpress starter theme. Modern and yet simple development workflow, made to quickly kickstart well organized & easy to maintain Wordpress themes. Webpack based setup supports Sass (scss) stylesheets, ECMAScript 2015+ compiling, Live reloading during development, image optimization, automatic built-in error logs, code linting and a lot more.",
  "main": "src/styles/app.js",
  "license": "ISC",
  "repository": "https://github.com/CatinhoCR/freshxmind-starter-theme",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config ./build-tasks/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config ./build-tasks/webpack.prod.js"
  },
  "author": "Cato",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.20.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.0",
    "imports-loader": "^0.8.0",
    "lint-staged": "^10.0.6",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-webpack-plugin": "^1.2.3",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "babel-polyfill": "^6.26.0",
    "modernizr": "^3.8.0",
    "swiper": "^6.4.11"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "last 6 iOS versions"
  ]
}
